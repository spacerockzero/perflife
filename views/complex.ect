<!-- complex.ect -->
<% extend 'layouts/layout.ect' %>

<% if @data?.length : %>
  <table class="complex-table">
    <% for item in @data : %>
      <tr>
        <td><%- item.name %></td>
        <td><%- item.catchPhrase %></td>
        <td><%- item.avatar %></td>
        <td><%- item.image %></td>
        <td><%- item.animal %></td>
        <td><%- item.company %></td>
      <tr>
    <% end %>
  </table>
<% else : %>
  <p>List is empty</p>
<% end %>
